CREATE SCHEMA `usuariosweb` ;

CREATE TABLE `usuariosweb`.`usuarios` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(20) NOT NULL,
  `apellido` VARCHAR(20) NOT NULL,
  `age` INT NOT NULL,
  `email` VARCHAR(60) NOT NULL,
  `password` VARCHAR(15) NOT NULL,
  PRIMARY KEY (`id`));


INSERT INTO usuariosweb.usuarios
(id, name, lastname, age, email, user, password)
VALUES
(0,'Carlos','Orellana',20,'robertosiete2@gmail.com','carlos123');

CREATE TABLE `usuariosweb`.`imagenes` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(145) NOT NULL,
  `tipo` VARCHAR(145) NOT NULL,
  `image` VARCHAR(145) NOT NULL,
  PRIMARY KEY (`id`));

SELECT * FROM usuariosweb.imagenes;

